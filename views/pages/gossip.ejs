<div>
    <% for(var i=0;i<gossips.length;i++) { %>
        <div class="gossip">
            <div class="content" style="font-size:12px;color:grey;word-wrap:break-word;">
                <%= gossips[i].gossip %>
            </div>
            <div class="timestamp" style="text-align:right;font-size:10px;color:grey;">
                <%= gossips[i].timestamp %>
            </div>
        </div>
    <% } %>
</div>
